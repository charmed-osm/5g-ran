#!/bin/sh

pdusessionid=`action-get pdusessionid`
datanetworkname=`action-get datanetworkname`
uupdumacaddress=`action-get uepdumacaddress`
sst=`action-get sst`
sd=`action-get sd`

curl --location --request POST 'http://10.45.28.62:8081/pdusessions/1' \
--header 'Content-Type: application/json' \
--header 'cart-id: 9' \
--data-raw "[
{
\"pdusessionid\": $pdusessionid,
\"datanetworkname\": \"$datanetworkname\",
\"uEPDUMacAddress\": \"$uepdumacaddress\",
\"snssai\":
{ \"sst\": \"$sst\", \"sd\": \"$sd\" }
}
]
"
